{
  "name": "plumber",
  "version": "0.0.0",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "/js/_base.js",
      "/js/background.js"
    ]
  },
  "browser_action": {
    "default_popup": "/html/browser_action.html"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "<all_urls>"
  ]
}